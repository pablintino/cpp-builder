{
  "tools": {
    "base-path": "/tools",
    "components": {
      "gcc-10.3": {
        "group": "compilers",
        "type": "gcc-build",
        "url": "https://github.com/gcc-mirror/gcc/archive/refs/tags/releases/gcc-10.3.0.tar.gz",
        "languages": [
          "c",
          "c++",
          "lto"
        ],
        "suffix-version": true,
        "config-opts": [
          "--enable-checking=release",
          "--disable-multilib"
        ]
      },
      "cmake-3.22": {
        "type": "cmake-build",
        "url": "https://github.com/Kitware/CMake/releases/download/v3.22.1/cmake-3.22.1.tar.gz"
      }
    }
  }
}